<!doctype html>
<html {% if section %} data-current="{{ section }}"{% endif %} lang="en">

	{% include "components/head.njk" %}

	<body>
		{% include "components/preloader.njk" %}

		{% include "components/search.njk" %}

		{% include "components/headerwrapper.njk" %}

		<main>
			{{ layoutContent | safe }}
		</main>

		{% include "components/footer.njk" %}
		
		{% set js %}
		{% include "assets/js/imagesloaded.min.js" %}
    {% include "assets/js/jquery-1.12.3.min.js" %}
		{% include "assets/js/popper.min.js" %}
		{% include "assets/js/plugin.js" %}
		{% include "assets/js/main.js" %}
  	{% endset %}
  	<script>{{ js | jsmin | safe }}</script>
		<script>window.twttr = (function(d, s, id) {
			var js, fjs = d.getElementsByTagName(s)[0],
				t = window.twttr || {};
			if (d.getElementById(id)) return t;
			js = d.createElement(s);
			js.id = id;
			js.src = "https://platform.twitter.com/widgets.js";
			fjs.parentNode.insertBefore(js, fjs);

			t._e = [];
			t.ready = function(f) {
				t._e.push(f);
			};

			return t;
		}(document, "script", "twitter-wjs"));</script>
	</body>

</html>
